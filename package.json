  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest",
    "build": "vite build",
    "lint": "run-p lint:type-check \"lint:eslint {@}\" --",
