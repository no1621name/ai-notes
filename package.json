    "lint:eslint": "eslint . --fix",
    "prepare": "husky",
    "supabase:deploy-functions": "eval $(grep '^SUPABASE_PROJECT_ID' .env) && supabase functions deploy --project-id $SUPABASE_PROJECT_ID",
    "supabase:generate": "eval $(grep '^SUPABASE_PROJECT_ID' .env) && supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > src/shared/api/supabase/supabase-generated.types.ts && eslint src/shared/api/supabase/supabase-generated.types.ts --fix"
  },
